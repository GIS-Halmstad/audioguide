{
  "name": "audioguide",
  "private": true,
  "version": "1.1.0",
  "description": "Audioguide",
  "license": "MIT",
  "author": "Jacob WodzyÅ„ski",
  "homepage": "https://github.com/GIS-Halmstad/audioguide#readme",
  "repository": "https://github.com/GIS-Halmstad/audioguide.git",
  "bugs": {
    "url": "https://github.com/GIS-Halmstad/audioguide/issues"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "dom7": "^4.0.6",
    "framework7": "^8.3.3",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.3",
    "material-icons": "^1.13.12",
    "ol": "^9.1.0",
    "plausible-tracker": "^0.3.8",
    "proj4": "^2.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^11.1.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/proj4": "^2.5.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-preset-env": "^9.5.9",
    "vite": "^5.2.10"
  }
}